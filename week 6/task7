public class eg7 {
    public static void main(String[] args) {
        int i=0,j=5,sum=0;
        for (i=0;i<10;i++){
            sum+=i;
        }
        if (i<j)
            System.out.println(i);
        else
            System.out.println(j);
        while (j<10){
            j++;
        }
        do {
            j++;
        }while (j<10);
    }
}
